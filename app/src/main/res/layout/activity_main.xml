<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <EditText
            android:layout_width="209dp"
            android:layout_height="74dp"
            android:text="Surname"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.173"
            app:layout_constraintVertical_bias="0.665" android:id="@+id/surname"/>
    <EditText
            android:layout_width="209dp"
            android:layout_height="74dp"
            android:text="First name"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.173"
            app:layout_constraintVertical_bias="0.523" android:id="@+id/firstname"/>
    <EditText
            android:layout_width="209dp"
            android:layout_height="74dp"
            android:text="Password"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.173"
            app:layout_constraintVertical_bias="0.372" android:id="@+id/password"/>
    <EditText
            android:layout_width="209dp"
            android:layout_height="74dp"
            android:text="E - mail"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.173"
            app:layout_constraintVertical_bias="0.223" android:id="@+id/email"/>
    <EditText
            android:layout_width="209dp"
            android:layout_height="74dp"
            android:text="Username"
            android:id="@+id/user"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.173"
            app:layout_constraintVertical_bias="0.082" tools:ignore="OnClick"/>

    <Button
            android:text="Register"
            android:layout_width="141dp"
            android:layout_height="72dp" android:id="@+id/button"
            android:layout_marginTop="46dp"
            app:layout_constraintTop_toBottomOf="@+id/surname" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="135dp" android:layout_marginEnd="135dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:onClick="change"
            android:layout_marginBottom="102dp" tools:ignore="OnClick"/>

</androidx.constraintlayout.widget.ConstraintLayout>